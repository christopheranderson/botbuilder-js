{
  "name": "botbuilder-azure",
  "author": "Microsoft Corp.",
  "description": "Azure extensions for Microsoft BotBuilder.",
  "version": "4.1.6",
  "license": "MIT",
  "keywords": [
    "botbuilder",
    "botframework",
    "bots",
    "chatbots",
    "azure"
  ],
  "bugs": {
    "url": "https://github.com/Microsoft/botbuilder-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/botbuilder-js.git"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "test": "tsc && nyc mocha tests/",
    "build": "tsc",
    "build-docs": "typedoc --theme markdown --entryPoint botbuilder-azure --excludePrivate --includeDeclarations --ignoreCompilerErrors --module amd --out ..\\..\\doc\\botbuilder-azure .\\lib\\index.d.ts --hideGenerator --name \"Bot Builder SDK - Azure\" --readme none",
    "clean": "erase /q /s .\\lib",
    "set-version": "npm version --allow-same-version ${Version}"
  },
  "files": [
    "/lib",
    "/src"
  ],
  "dependencies": {
    "@azure/cosmos": "^3.3.1",
    "@types/documentdb": "^1.10.5",
    "@types/node": "^10.12.18",
    "azure-storage": "2.10.2",
    "botbuilder": "4.1.6",
    "documentdb": "1.14.5",
    "flat": "^4.0.0",
    "semaphore": "^1.1.0"
  },
  "devDependencies": {
    "@azure/ms-rest-js": "^1.8.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^2.2.47",
    "@types/semaphore": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "codelyzer": "^4.1.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "fs-extra": "~8.1.0",
    "mocha": "^6.1.4",
    "ms-rest-azure": "^2.6.0",
    "nock": "^10.0.3",
    "nyc": "^11.4.1",
    "read-text-file": "^1.1.0",
    "replace-in-file": "^4.1.0",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^2.2.10",
    "typescript": "3.5.2"
  }
