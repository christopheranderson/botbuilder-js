{
  "name": "botbuilder-core",
  "author": "Microsoft Corp.",
  "description": "Core components for Microsoft Bot Builder. Components in this library can run either in a browser or on the server.",
  "version": "4.1.6",
  "license": "MIT",
  "keywords": [
    "botbuilder",
    "botframework",
    "bots",
    "chatbots"
  ],
  "bugs": {
    "url": "https://github.com/Microsoft/botbuilder-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/botbuilder-js.git"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "test": "tsc && nyc mocha tests/",
    "build": "tsc",
    "build-docs": "typedoc --theme markdown --entryPoint botbuilder --excludePrivate --includeDeclarations --ignoreCompilerErrors --module amd --out ..\\..\\doc\\botbuilder .\\lib\\index.d.ts ..\\botbuilder-core\\lib\\index.d.ts ..\\botframework-schema\\lib\\index.d.ts --hideGenerator --name \"Bot Builder SDK\" --readme none",
    "clean": "erase /q /s .\\lib",
    "set-version": "npm version --allow-same-version ${Version}"
  },
  "files": [
    "/lib",
    "/src"
  ],
  "dependencies": {
    "assert": "^1.4.1",
    "botframework-schema": "4.1.6"
  },
  "devDependencies": {
    "@azure/ms-rest-js": "^1.8.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^2.2.47",
    "@types/node": "^12.6.9",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "mocha": "^6.1.4",
    "ms-rest-azure": "^2.6.0",
    "nyc": "^11.4.1",
    "read-text-file": "^1.1.0",
    "replace-in-file": "^4.1.0",
    "request": "~2.88.0",
    "rimraf": "~3.0.0",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^2.2.10",
    "typescript": "3.5.2",
    "unzip": "^0.1.11"
  }
}
